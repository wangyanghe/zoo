page,.buy {
    width: 100%;
    background: #F2F2F2;
    position: relative;
}

.buy-header {
    background: #fff;
    margin-bottom: 20rpx;
}

.header-name {
    display: flex;
    padding: 32rpx 29rpx 32rpx 32rpx;
    border-bottom: 1rpx solid #E3E3E3;
}

.img,.img-desc {
    width: 134rpx;
    height: 134rpx;
    border-radius: 10rpx;
}

.header-desc {
    flex: 1;
    padding-left: 30rpx;
}

.desc-list {
    font-size: 28rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #333333;
    line-height: 40rpx;
    margin-bottom: 35rpx;
}

.desc-price {
    display: flex;
}

.price-num {
    width: 100rpx;
    font-size: 24rpx;
    font-family: SIL-Hei-Med-Jian;
    font-weight: 400;
    color: #FF3300;
}

.price-total {
    font-size: 20rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #999999;
    flex: 1;
    text-align: right;
}

.price-total {
    font-size: 20rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
}

.header-express {
    margin: 0 28rpx;
    border-bottom: 1rpx solid #E3E3E3;
    height: 26rpx;
    font-size: 28rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    padding: 30rpx 0;
    display: flex;
    background: #fff;
}

.express-title {
    font-size: 28rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    width: 120rpx;
}

.express-price {
    flex: 1;
    text-align: right;
}

.header-num {
    height: 72rpx;
    line-height: 72rpx;
    border-bottom: 1rpx solid #E3E3E3;
    display: flex;
    background: #fff;
    padding: 14rpx 30rpx 12rpx;
}

.num-name {
    font-size: 28rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #262626;
    width: 120rpx;
}

.num-edite {
    flex: 1;
    text-align: right;
}

.less {
    width: 68rpx;
    height: 68rpx;
    line-height: 68rpx;
    text-align: center;
    border: 2rpx solid #ECECEC;
    color: #ABABAD;
    float: right;
}

.number-less {
    width: 98rpx;
    height: 68rpx;
    line-height: 68rpx;
    border-bottom: 2rpx solid #ECECEC;
    border-top: 2rpx solid #ECECEC;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #262626;
    float: right;
    text-align: center;
}

.less.add {
    color: #FF753A;
}

.header-total {
    padding: 44rpx 30rpx 30rpx;
}

.header-total {
    font-size: 26rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #333333;
    text-align: right;
}

.header-total .total {
    color: #FF3300;
    font-size: 24rpx;
    font-family: SIL-Hei-Med-Jian;
    font-weight: bold;
}

.buy-info {
    background: #fff;
    margin-bottom: 160rpx;
    z-index: 0!important;
}

.inf0-map {
    padding: 60rpx 32rpx 35rpx;
    display: flex;
}

.map {
    margin-right: 20rpx;
    width: 29rpx;
    height: 33rpx;
}

.info-list {
    padding: 0 32rpx;
}

.info-img {
    width: 29rpx;
    height: 33rpx;
}

.info-header {
    font-size: 34rpx;
    font-family: SourceHanSansCN-Bold;
    font-weight: bold;
    color: #262626;
    flex: 1;
}

.info-name {
    display: flex;
    height: 99rpx;
    line-height: 99rpx;
}

.name-list {
    width: 135rpx;
    height: 31rpx;
    line-height: 31rpx;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #262626;
    margin-top: 35rpx;
    margin-bottom: 31rpx;
    margin-right: 80rpx;
}

.input-details {
    flex: 1;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #A6A6A6;
    margin-top: 18rpx;
}

.buy-pay {
    background: #fff;
    border-top: 1rpx solid #eee;
    box-sizing: border-box;
    height: 98rpx;
    line-height: 98rpx;
    width: 100%;
    position: fixed;
    bottom: 0;
    z-index: 2000!important;
}

.buy-pay .total {
    float: left;
    padding-left: 27rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #262626;
}

.buy-pay .money {
    font-size: 30rpx;
    font-family: SIL-Hei-Med-Jian;
    font-weight: bold;
    color: #FF3300;
}

.pay-title {
    width: 350rpx;
    background: linear-gradient(90deg,rgba(255,153,51,1),rgba(255,110,59,1));
    color: #fff;
    font-size: 36rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    float: right;
    text-align: center;
}

.textarea-item {
    border: 1rpx solid #f3f4f7;
    margin-top: 35rpx;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(166,166,166,1);
    padding-top: 10rpx;
    padding-left: 10rpx;
    z-index: 0!important;
    flex: 1;
    min-height: 42px;
    height: auto;
}

.border-bottom {
    height: 6rpx!important;
    width: 100%;
}

.valider {
    color: red;
    padding-left: 200rpx;
    padding-bottom: 15rpx;
}

.info-name.title {
    height: auto;
    line-height: auto;
    border-bottom: 0;
}

.container {
    border-bottom: 2rpx solid #f3f4f7;
}

.input-box {
    display: flex;
    align-items: center;
}

.input-box .name-list {
    width: 235rpx;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #262626;
}

.input-box .input {
    width: 100%;
    height: 31rpx;
    line-height: 31rpx;
    font-size: 32rpx;
    color: #646464;
    padding: 0 10rpx;
}

.city-picker-mask {
    position: fixed;
    height: 100%;
    width: 100%;
    min-height: 100%;
    background-color: rgba(0,0,0,.5);
    z-index: 160;
    bottom: 0;
    left: 0;
}

.city-picker-mask .city-picker-inner {
    position: absolute;
    left: 0;
    bottom: 0;
    background-color: #fff;
    width: 100%;
}

.city-picker-inner .citybody {
    width: 100%;
}

.city-picker-inner .city-header {
    box-sizing: border-box;
    width: 100%;
    height: 30px;
    line-height: 30px;
    z-index: 4;
    padding: 0 20rpx;
    background-color: #fff;
}

.city-header .city-cancel {
    float: left;
    color: #818181;
}

.city-header .city-true {
    float: right;
    color: #2FB42E;
}

.city-picker-inner .picker-column-item {
    width: 100%;
    color: #646464;
    font-size: 28rpx;
}