page,.goods {
    width: 100%;
    background: #F2F2F2;
    position: relative;
}

.good-img,.good-imgs,.swiper-list {
    width: 100%;
    height: 370rpx;
}

.goods-list {
    width: 100%;
    background: #fff;
    margin-bottom: 20rpx;
}

.swiper-imgaes-list {
    height: 370rpx;
    width: 100%;
}

.goods-des {
    padding: 38rpx 25rpx 27rpx 32rpx;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Medium;
    font-weight: 500;
    color: #333333;
    line-height: 40rpx;
}

.goods-price {
    display: flex;
    padding: 0rpx 25rpx 31rpx 32rpx;
}

.price-num {
    width: 120rpx;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 30rpx;
    font-family: SIL-Hei-Med-Jian;
    font-weight: bold;
    color: #FF3300;
}

.price-total {
    font-size: 24rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #999999;
    flex: 1;
    text-align: right;
}

.total-header {
    padding-left: 20rpx;
}

.goods-express {
    border-top: 1px solid #E3E3E3;
    padding: 27rpx 30rpx 30rpx;
}

.express {
    width: 30rpx;
    height: 30rpx;
    margin-right: 12rpx;
    vertical-align: bottom;
}

.stock {
    width: 22rpx;
    height: 22rpx;
    margin-right: 12rpx;
    vertical-align: bottom;
}

.express-dec {
    font-size: 24rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #666666;
}

.express-list,.express-stock {
    display: inline-block;
    margin-right: 55rpx;
}

.goods-details {
    padding: 30rpx;
    background: #fff;
}

.border-width {
    height: 29rpx;
    line-height: 29rpx;
    font-size: 30rpx;
    font-family: SourceHanSansCN-Medium;
    font-weight: 500;
    padding-left: 15rpx;
    border-left: 8rpx solid #FF753A;
    border-radius: 4rpx;
}

.details-item {
    width: 100%;
    height: 350rpx;
}

.details-desc {
    height: 29rpx;
    line-height: 29rpx;
    font-size: 30rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #333333;
}

.details-pay {
    height: 98rpx;
    line-height: 98rpx;
    background: #FFFFFF;
    box-shadow: 0rpx 0rpx 7rpx 0rpx rgba(0,0,0,0.14);
    position: fixed;
    bottom: 0;
    width: 100%;
}

.service-img {
    width: 44rpx;
    height: 43rpx!important;
    display: block;
    margin-bottom: 10rpx;
}

.servicer {
    font-size: 20rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #666666;
    height: 19rpx;
    line-height: 19rpx;
    display: block;
}

.service-title {
    padding: 15rpx 32rpx;
    float: left;
}

.right-width {
    margin: 15rpx 0;
    float: left;
    height: 68rpx;
    border: 1rpx solid #f3f3f3;
}

.pay {
    width: 350rpx;
    height: 98rpx;
    line-height: 98rpx;
    text-align: center;
    background: linear-gradient(90deg,rgba(255,153,51,1),rgba(255,110,59,1));
    font-size: 36rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: #fff;
    float: right;
}

.swiper-list {
    width: 100%;
    box-sizing: border-box;
    height: 375rpx;
}

.swiper-header {
    width: 100%!important;
    height: 100%!important;
}

.swiper-list .a-swiper-slides {
    height: 375rpx!important;
    width: 100%!important;
}

.swiper-list .swiper {
    height: 375rpx!important;
    width: 100%!important;
}

.swiper-list .index-list {
}

.swiper_container {
    height: 55rpx;
    width: 100%;
}

.swiper-imgaes-list {
    width: 100%;
    height: 0;
    overflow: hidden;
    background-position: center center;
    background-size: contain;
    background-size: cover!important;
    padding-right: 20rpx;
}

.swiper_item {
    font-size: 25rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    letter-spacing: 4rpx;
}

.swiper-list .a-swiper-indicator {
    bottom: 15rpx;
}

.swiper-list .a-image {
    width: 750rpx!important;
    height: 375rpx!important;
}

.swiper-list-bg {
    position: absolute;
    top: 330rpx;
    left: 0;
    height: 50rpx;
    width: 100%;
    background-size: contain;
    background-size: 100%;
}

.swiper-item {
    width: 100%;
    height: 100%;
}

.service-dialog {
    background: rgba(0,0,0,0.5);
    top: 0;
    bottom: 0;
    height: 100%;
    position: fixed;
    width: 100%;
}

.service {
    width: 496rpx;
    height: 260rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0rpx 1rpx 8rpx 0rpx rgba(51,51,51,0.06);
    border-radius: 15rpx;
    position: absolute;
    left: 50%;
    bottom: 50%;
    margin-left: -248rpx;
    margin-top: 130rpx;
}

.serviec-phone {
    height: 190rpx;
    text-align: center;
}

.serviec-btn {
    height: 70rpx;
    border-top: 1rpx solid #EEEEEE;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.service-back {
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(51,51,51,1);
    width: 48%;
    border-right: 1rpx solid #eee;
    height: 68rpx;
    line-height: 68rpx;
    text-align: center;
}

.service-back.config {
    border-right: 0;
}

.service-name {
    padding-top: 57rpx;
    font-size: 30rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
}

.service-header {
    margin-bottom: 22rpx;
}